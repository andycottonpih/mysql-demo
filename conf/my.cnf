[mysqld]
explicit_defaults_for_timestamp=1
character-set-server=utf8
collation-server=utf8_general_ci
max_allowed_packet=1G
innodb-buffer-pool-size=2G
user=mysql
server-id=1
binlog_format=ROW
max_binlog_size=100M
#Uncomment after first start if needed, container cannot initialize data folder with this option enabled.
#log-bin=mysql-bin